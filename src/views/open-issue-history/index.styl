
.open-issue-history-view {
  &.k3 {
    dice() {
      display inline-block
      background-size 100%
      margin-right 0.04rem
      border-radius 0
    }
    for num in 1 2 3 4 5 6 {
      .n-{num} {
        background url("../../assets/images/default/dice-" + num + ".png") center no-repeat
        dice()
      }
    }
  }
}